#include<stdio.h>

void Troca (int *x, int *y);

int main (){
    int n1, n2;

    printf("Digite os nï¿½meros: ");
    scanf("%d %d", &n1, &n2);
    Troca(&n1, &n2);
    printf("Valores trocados: %d %d", n1, n2);

    return 0;
}

void Troca (int *x, int *y){
    int aux;

    aux = *x;
    *x = *y;
    *y = aux;
}
